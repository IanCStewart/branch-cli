{
	"name": "branch-cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "xo",
		"pretty": "prettier --check .",
		"typecheck": "tsc -p tsconfig.json",
		"test": "ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "1.0.0",
		"ink": "4.2.0",
		"meow": "12.0.1",
		"react": "18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "3.0.1",
		"@types/react": "18.2.7",
		"@vdemedes/prettier-config": "2.0.1",
		"ava": "^5.3.0",
		"chalk": "5.2.0",
		"eslint-config-xo-react": "0.27.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"ink-testing-library": "3.0.0",
		"prettier": "2.8.8",
		"ts-node": "10.9.1",
		"typescript": "5.0.4",
		"xo": "0.54.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
		}
	}
}
